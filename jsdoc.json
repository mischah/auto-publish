{
  "source": {
    "include": ["README.md", "index.js"],
    "includePattern": ".+\\.js(doc)?$"
  },
  "opts": {
    "recurse": true,
    "destination": "../yeoman-generator-doc",
    "package": "package.json"
  },
  "templates": {
    "default": {
      "includeDate": false
    }
  },
  "plugins": [
    "plugins/markdown"
  ]
}
